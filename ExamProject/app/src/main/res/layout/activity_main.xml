<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="145dp"
        tools:layout_editor_absoluteY="135dp">

        <Button
            android:id="@+id/buttonTrinarnaLogic"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickTrinarnaLogic"
            android:text="Trinarna logic " />

        <Button
            android:id="@+id/buttonCars"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickCars"
            android:text="Cars" />

        <Button
            android:id="@+id/buttonDeveloperInformation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="onClickDeveloperInformation"
            android:text="developer information" />

        <ImageButton
            android:id="@+id/imageButtonInfo"
            android:layout_width="100px"
            android:layout_height="100px"
            android:textAlignment="center"
            app:srcCompat="@android:drawable/ic_menu_info_details" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>